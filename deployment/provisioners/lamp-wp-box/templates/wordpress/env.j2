DB_NAME={{ application_db_name }}
DB_USER={{ application_db_user }}
DB_PASSWORD={{ application_db_password }}
DB_HOST={{ application_db_host }}

WP_ENV={{ env }}
WP_HOME={{application_wp_home}}
WP_SITEURL={{application_wp_home}}/

AUTH_KEY="{{ auth_key }}"
SECURE_AUTH_KEY="{{ secure_auth_key }}"
LOGGED_IN_KEY="{{ logged_in_key }}"
NONCE_KEY="{{ nonce_key }}"
AUTH_SALT="{{ auth_salt }}"
SECURE_AUTH_SALT="{{ secure_auth_salt }}"
LOGGED_IN_SALT="{{ logged_in_salt }}"
NONCE_SALT="{{ nonce_salt }}"

{% for key in application_env|default({}) %}
{{key}}={{application_env[key]}}
{% endfor %}
